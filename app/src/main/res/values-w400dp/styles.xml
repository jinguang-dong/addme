<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MaterialButtonFilledStyle" parent="@style/Widget.GoogleMaterial3.Button">
        <item name="android:textAppearance">?attr/textAppearanceButton</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:layout_gravity">0x1</item>
        <item name="android:layout_width">-2</item>
        <item name="android:layout_height">@dimen/bottom_sheet_button_height</item>
        <item name="android:minWidth">@dimen/bottom_sheet_button_width</item>
        <item name="backgroundTint">?attr/colorPrimary</item>
    </style>
    <style name="MaterialButtonOutlinedStyle" parent="@style/Widget.GoogleMaterial3.Button.OutlinedButton">
        <item name="android:textAppearance">?attr/textAppearanceButton</item>
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:layout_gravity">0x1</item>
        <item name="android:layout_width">-2</item>
        <item name="android:layout_height">@dimen/bottom_sheet_button_height</item>
        <item name="android:minWidth">@dimen/bottom_sheet_button_width</item>
        <item name="strokeColor">?attr/colorPrimary</item>
    </style>
</resources>
