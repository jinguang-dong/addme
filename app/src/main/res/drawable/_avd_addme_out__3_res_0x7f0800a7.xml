<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt" android:ordering="together">
    <objectAnimator
        android:interpolator="@anim/ease_out"
        android:duration="@integer/duration_spatial_exit"
        android:startOffset="0"
        android:valueFrom="0"
        android:valueTo="45"
        android:valueType="floatType"
        android:propertyName="rotation"/>
    <objectAnimator
        android:interpolator="@anim/ease_out"
        android:duration="@integer/duration_spatial_exit"
        android:startOffset="0"
        android:valueFrom="0"
        android:valueTo="-6"
        android:valueType="floatType"
        android:propertyName="translateX"/>
    <objectAnimator
        android:interpolator="@anim/ease_out"
        android:duration="@integer/duration_spatial_exit"
        android:startOffset="0"
        android:valueFrom="0"
        android:valueTo="6"
        android:valueType="floatType"
        android:propertyName="translateY"/>
</set>
